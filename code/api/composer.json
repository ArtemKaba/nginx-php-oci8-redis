{
    "name": "lumen with oci8",
    "description": "API",
    "authors": [
        {
            "name": "Eakkapat Pattarathamrong",
            "email": "overbid@gmail.com",
            "homepage": "https://github.com/overbid",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.1",
        "laravel/lumen-framework": "5.4.*",
        "vlucas/phpdotenv": "~2.2",
        "tymon/jwt-auth": "1.0.0-beta.2",
        "yajra/laravel-oci8": "5.4.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9",
        "friendsofphp/php-cs-fixer": "*",
        "codeception/codeception": "*",
        "wn/lumen-generators": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}